{
  "version": 2,
  "buildCommand": "echo 'No build needed - static files only'",
  "outputDirectory": "FIRM-Core/FIRM_ui",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}

