{
  "version": 2,
  "installCommand": "echo 'No dependencies to install'",
  "buildCommand": "mkdir -p FIRM_ui && cp -r FIRM-Core/FIRM_ui/* FIRM_ui/",
  "outputDirectory": "FIRM_ui",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}

