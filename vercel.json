{
  "version": 2,
  "name": "firm-exnihilo",
  "builds": [
    {
      "src": "FIRM-Core/FIRM_ui/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/FIRM-Core/FIRM_ui/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/FIRM-Core/FIRM_ui/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}

