{
  "rewrites": [
    { "source": "/_vercel/(.*)", "destination": "/_vercel/$1" },
    { "source": "/(.*)", "destination": "/FIRM-Core/FIRM_ui/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
